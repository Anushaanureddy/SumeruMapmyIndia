<?xml version="1.0" encoding="utf-8"?>
<plugin id="cordova-plugin-sumerumapmyindia" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
	<name>
		SumeruMapmyIndia
	</name>
	<js-module name="SumeruMapmyIndia" src="www/SumeruMapmyIndia.js">
		<clobbers target="SumeruMapmyIndia" />
	</js-module>
	<platform name="android">
		<config-file parent="/*" target="res/xml/config.xml">
			<feature name="SumeruMapmyIndia">
				<param name="android-package" value="cordova.plugin.sumerumapmyindia.SumeruMapmyIndia" />
			</feature>
		</config-file>
		<config-file parent="/*" target="AndroidManifest.xml" />
		<source-file src="src/android/SumeruMapmyIndia.java" target-dir="src/cordova/plugin/sumerumapmyindia/SumeruMapmyIndia" />
	</platform>
	<platform name="ios">
		<config-file parent="/*" target="config.xml">
			<feature name="SumeruMapmyIndia">
				<param name="ios-package" value="SumeruMapmyIndia" />
			</feature>
		</config-file>
		<source-file src="src/ios/SumeruMapmyIndia.m" />
	</platform>
</plugin>
